def arraySum(lst):
    # Python program to add all the array elements using the built-in function
    return(sum(lst))


def arrayProduct(lst):
    # Multiply elements one by one
    product = 1
    for x in lst:
        product = product * x
    return product
    
def reverseArray(lst):
    result=list(reversed(lst))
    return result    

def main():
    lst = []
    n = int(input("Enter the no.of elements : "))
    for i in range(n):
        ele = int(input())
        lst.append(ele)
    print(str(arraySum(lst)))
    print(str(arrayProduct(lst)))

if __name__=="__main__":
    main()
